<%= form_for @location do |f| %>

  <p>
    <%= f.label :address %><br />
    <%= f.text_field :address %>
  </p>
  <p>
    <%= f.label :latitude %><br />
    <%= f.text_field :latitude %>
  </p>
  <p>
    <%= f.label :longitude %><br />
    <%= f.text_field :longitude %>
  </p>
 <p>
    <%= f.label :hashtag %><br />
    <%= f.text_field :hashtag %>
  </p>

  <p><%= f.submit %></p>
<% end %>
